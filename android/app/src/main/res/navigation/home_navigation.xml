            app:destination="@id/dateFilterPickerBottomSheet" />
        <action
            android:id="@+id/action_accountDetailFragment_to_showQrBottomSheet"
            app:destination="@id/showQrBottomSheet" />
        <action
            android:id="@+id/action_accountDetailFragment_to_assetSelectionFragment"
