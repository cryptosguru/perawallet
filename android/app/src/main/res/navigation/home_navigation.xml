<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/homeNavigation"
    app:startDestination="@id/accountsFragment">

    <include app:graph="@navigation/wallet_connect_navigation" />

    <include app:graph="@navigation/single_button_bottom_sheet_navigation" />

    <include app:graph="@navigation/send_algo_navigation" />

    <include app:graph="@navigation/qr_scanner_navigation" />

    <include app:graph="@navigation/warning_confirmation_navigation" />

    <action
        android:id="@+id/action_global_send_algo_navigation"
        app:destination="@id/sendAlgoNavigation">
        <argument
            android:name="assetTransaction"
            app:argType="com.algorand.android.models.AssetTransaction"
            app:nullable="true" />
    </action>

    <include app:graph="@navigation/single_button_bottom_sheet_navigation" />

    <action
        android:id="@+id/action_global_walletConnectConnectionBottomSheet"
        app:destination="@id/walletConnectConnectionBottomSheet" />

    <action
        android:id="@+id/action_global_unsupportedAssetNotificationRequestActionBottomSheet"
        app:destination="@id/unsupportedAssetNotificationRequestActionBottomSheet" />

    <action
        android:id="@+id/action_global_ledgerConnectionIssueBottomSheet"
        app:destination="@id/ledgerConnectionIssueBottomSheet" />

    <action
        android:id="@+id/action_global_unsupportedAddAssetTryLaterBottomSheet"
        app:destination="@id/unsupportedAddAssetTryLaterBottomSheet" />

    <action
        android:id="@+id/action_global_unsupportedAssetInfoActionBottomSheet"
        app:destination="@id/unsupportedAssetInfoActionBottomSheet" />

    <action
        android:id="@+id/action_global_addContactFragment"
        app:destination="@id/addContactFragment" />

    <action
        android:id="@+id/action_global_assetDetailFragment"
        app:destination="@id/assetDetailFragment" />

    <action
        android:id="@+id/action_global_receiveAccountSelectionFragment"
        app:destination="@id/receiveAccountSelectionFragment" />

    <action
        android:id="@+id/action_global_showQrBottomSheet"
        app:destination="@id/showQrBottomSheet" />

    <action
        android:id="@+id/action_global_walletConnectRequestNavigation"
        app:destination="@id/walletConnectRequestNavigation"
        app:popUpTo="@id/walletConnectRequestNavigation"
        app:popUpToInclusive="true" />

    <!-- Remove this action and use the one in the main navigation after Lock Screen navigation change. -->
    <action
        android:id="@+id/action_global_singleButtonBottomSheet"
        app:destination="@id/singleButtonBottomSheetNavigation">
        <argument
            android:name="titleAnnotatedString"
            app:argType="com.algorand.android.models.AnnotatedString" />
        <argument
            android:name="descriptionAnnotatedString"
            app:argType="com.algorand.android.models.AnnotatedString" />
        <argument
            android:name="drawableResId"
            android:defaultValue="0"
            app:argType="reference" />
        <argument
            android:name="drawableTintResId"
            android:defaultValue="0"
            app:argType="reference" />
        <argument
            android:name="isResultNeeded"
            android:defaultValue="false"
            app:argType="boolean" />
    </action>

    <fragment
        android:id="@+id/contactsFragment"
        android:name="com.algorand.android.ui.contacts.ContactsFragment"
        android:label="fragment_contacts"
        tools:layout="@layout/fragment_contacts">
        <action
            android:id="@+id/action_contactsFragment_to_showQrBottomSheet"
            app:destination="@id/showQrBottomSheet" />
        <action
            android:id="@+id/action_contactsFragment_to_contactInfoFragment"
            app:destination="@id/contactInfoFragment" />
        <action
            android:id="@+id/action_contactsFragment_to_addContactFragment"
            app:destination="@id/addContactFragment" />
        <action
            android:id="@+id/action_contactsFragment_to_editContactFragment"
            app:destination="@id/editContactFragment" />
    </fragment>

    <dialog
        android:id="@+id/viewPassphraseLockBottomSheet"
        android:name="com.algorand.android.ui.accounts.ViewPassphraseLockBottomSheet"
        android:label="ViewPassphraseLockBottomSheet"
        tools:layout="@layout/bottom_sheet_view_passphrase_lock">
        <argument
            android:name="publicKey"
            app:argType="string" />
    </dialog>

    <fragment
        android:id="@+id/addAssetFragment"
        android:name="com.algorand.android.ui.addasset.AddAssetFragment"
        android:label="AddAssetFragment"
        tools:layout="@layout/fragment_add_asset">
        <argument
            android:name="accountPublicKey"
            app:argType="string" />
        <action
            android:id="@+id/action_addAssetFragment_to_accountsFragment"
            app:destination="@id/accountsFragment"
            app:launchSingleTop="true"
            app:popUpTo="@+id/mainNavigation"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_addAssetFragment_to_verifiedAssetInformationBottomSheet"
            app:destination="@id/verifiedAssetInformationBottomSheet" />
        <action
            android:id="@+id/action_addAssetFragment_to_addAssetActionBottomSheet"
            app:destination="@id/addAssetActionBottomSheet" />
    </fragment>

    <fragment
        android:id="@+id/accountsFragment"
        android:name="com.algorand.android.ui.accounts.AccountsFragment"
        android:label="fragment_accounts"
        tools:layout="@layout/fragment_accounts">
        <action
            android:id="@+id/action_accountsFragment_to_showQrBottomSheet"
            app:destination="@id/showQrBottomSheet" />
        <action
            android:id="@+id/action_accountsFragment_to_peraIntroductionFragment"
            app:destination="@id/peraIntroductionFragment" />
        <action
            android:id="@+id/action_accountsFragment_to_addAssetFragment"
            app:destination="@id/addAssetFragment" />
        <action
            android:id="@+id/action_accountsFragment_to_removeAssetsFragment"
            app:destination="@id/removeAssetsFragment" />
        <action
            android:id="@+id/action_accountsFragment_to_qrCodeScannerNavigation"
            app:destination="@id/qrCodeScannerNavigation">
            <argument
                android:name="qrScanner"
                app:argType="com.algorand.android.models.QrScanner" />
        </action>
        <action
            android:id="@+id/action_accountsFragment_to_accountErrorOptionsBottomSheet"
            app:destination="@id/accountErrorOptionsBottomSheet" />
        <action
            android:id="@+id/action_accountsFragment_to_viewPassphraseBottomSheet"
            app:destination="@id/viewPassphraseBottomSheet" />
        <action
            android:id="@+id/action_accountsFragment_to_homeAccountOptionsBottomSheet"
            app:destination="@id/homeAccountOptionsBottomSheet" />
        <action
            android:id="@+id/action_accountsFragment_to_accountDetailFragment"
            app:destination="@id/accountDetailFragment" />
        <action
            android:id="@+id/action_accountsFragment_to_standardAccountOrderFragment"
            app:destination="@id/standardAccountOrderFragment" />
        <action
            android:id="@+id/action_accountsFragment_to_watchAccountOrderFragment"
            app:destination="@id/watchAccountOrderFragment" />
        <action
            android:id="@+id/action_accountsFragment_to_notificationCenterFragment"
            app:destination="@id/notificationCenterFragment" />
    </fragment>

    <fragment
        android:id="@+id/removeAssetsFragment"
        android:name="com.algorand.android.ui.removeasset.RemoveAssetsFragment"
        android:label="fragment_remove_assets"
        tools:layout="@layout/fragment_remove_assets">
        <argument
            android:name="accountPublicKey"
            app:argType="string" />
        <action
            android:id="@+id/action_removeAssetsFragment_to_receiverAccountSelectionFragment"
            app:destination="@id/receiverAccountSelectionFragment"
            app:popUpTo="@id/accountsFragment" />
        <action
            android:id="@+id/action_removeAssetsFragment_to_removeAssetActionBottomSheet"
            app:destination="@id/removeAssetActionBottomSheet" />
        <action
            android:id="@+id/action_removeAssetsFragment_to_transferBalanceActionBottomSheet"
            app:destination="@id/transferBalanceActionBottomSheet" />
    </fragment>

    <fragment
        android:id="@+id/contactInfoFragment"
        android:name="com.algorand.android.ui.contacts.ContactInfoFragment"
        android:label="ContactInfoFragment"
        tools:layout="@layout/fragment_contact_info">
        <action
            android:id="@+id/action_editContactFragment_to_showQrBottomSheet"
            app:destination="@id/showQrBottomSheet" />
        <argument
            android:name="contact"
            app:argType="com.algorand.android.models.User" />
        <action
            android:id="@+id/action_contactInfoFragment_to_editContactFragment"
            app:destination="@id/editContactFragment"
            app:popUpTo="@id/contactInfoFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_contactInfoFragment_to_showQrBottomSheet"
            app:destination="@id/showQrBottomSheet" />
        <action
            android:id="@+id/action_contactInfoFragment_to_accountSelectionBottomSheet"
            app:destination="@id/accountSelectionBottomSheet" />
        <action
            android:id="@+id/action_contactInfoFragment_to_assetTransferAmountFragment"
            app:destination="@id/assetTransferAmountFragment" />
    </fragment>

    <fragment
        android:id="@+id/changePasswordFragment"
        android:name="com.algorand.android.ui.settings.ChangePasswordFragment"
        android:label="fragment_settings_change_password"
        tools:layout="@layout/fragment_base_password" />

    <dialog
        android:id="@+id/transactionDetailBottomSheet"
        android:name="com.algorand.android.ui.transactiondetail.TransactionDetailBottomSheet"
        android:label="TransactionDetailBottomSheet"
        tools:layout="@layout/bottom_sheet_transaction_detail">
        <argument
            android:name="transactionItem"
            app:argType="com.algorand.android.models.BaseTransactionItem" />
        <action
            android:id="@+id/action_transactionDetailBottomSheet_to_addContactFragment"
            app:destination="@id/addContactFragment" />
        <action
            android:id="@+id/action_transactionDetailBottomSheet_to_showQrBottomSheet"
            app:destination="@id/showQrBottomSheet" />
    </dialog>

    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.algorand.android.ui.settings.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settingsFragment_to_warningConfirmationNavigation"
            app:destination="@id/warningConfirmationNavigation">
            <argument
                android:name="warningConfirmation"
                app:argType="com.algorand.android.models.WarningConfirmation" />
        </action>
        <action
            android:id="@+id/action_settingsFragment_to_rateExperienceBottomSheet"
            app:destination="@id/rateExperienceBottomSheet" />
        <action
            android:id="@+id/action_settingsFragment_to_developerSettingsFragment"
            app:destination="@id/developerSettingsFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_currencySelectionFragment"
            app:destination="@id/currencySelectionFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_languageSelectionFragment"
            app:destination="@id/languageSelectionFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_themeSelectionFragment"
            app:destination="@+id/themeSelectionFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_notificationSettingsFragment"
            app:destination="@id/notificationSettingsFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_walletConnectSessionsFragment"
            app:destination="@id/walletConnectSessionsFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_securityFragment"
            app:destination="@id/securityFragment" />
    </fragment>

    <fragment
        android:id="@+id/securityFragment"
        android:name="com.algorand.android.ui.settings.SecurityFragment"
        android:label="SecurityFragment"
        tools:layout="@layout/fragment_security">
        <action
            android:id="@+id/action_securityFragment_to_changePasswordFragment"
            app:destination="@id/changePasswordFragment" />
    </fragment>

    <fragment
        android:id="@+id/notificationCenterFragment"
        android:name="com.algorand.android.ui.notificationcenter.NotificationCenterFragment"
        android:label="NotificationCenterFragment"
        tools:layout="@layout/fragment_notification_center">
        <action
            android:id="@+id/action_notificationCenterFragment_to_assetDetailFragment"
            app:destination="@id/assetDetailFragment" />
        <action
            android:id="@+id/action_notificationCenterFragment_to_notificationFilterFragment"
            app:destination="@id/notificationFilterFragment" />
    </fragment>

    <fragment
        android:id="@+id/rekeyInstructionFragment"
        android:name="com.algorand.android.ui.rekey.RekeyInstructionFragment"
        android:label="RekeyInstructionFragment"
