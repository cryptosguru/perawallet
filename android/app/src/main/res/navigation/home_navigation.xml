        <argument
            android:name="rekeyAddress"
            app:argType="string" />
        <action
            android:id="@+id/action_rekeyInstructionFragment_to_rekeyLedgerSearchFragment"
            app:destination="@id/rekeyLedgerSearchFragment" />
    </fragment>

    <fragment
        android:id="@+id/rekeyLedgerSearchFragment"
