        android:id="@+id/accountDetailFragment"
        android:name="com.algorand.android.ui.accountdetail.AccountDetailFragment"
        android:label="AccountDetailFragment">
        <argument
            android:name="publicKey"
            app:argType="string" />
        <action
            android:id="@+id/action_accountDetailFragment_to_accountOptionsBottomSheet"
            app:destination="@id/accountOptionsBottomSheet" />
        <action
            android:id="@+id/action_accountDetailFragment_to_viewPassphraseBottomSheet"
            app:destination="@id/viewPassphraseBottomSheet" />
        <action
            android:id="@+id/action_accountDetailFragment_to_transactionDetailBottomSheet"
            app:destination="@id/transactionDetailBottomSheet" />
