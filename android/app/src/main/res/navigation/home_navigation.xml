            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_accountOptionsBottomSheet_to_rekeyAccountFragment"
            app:destination="@id/rekeyInstructionFragment" />
        <action
            android:id="@+id/action_accountOptionsBottomSheet_to_showQrBottomSheet"
            app:destination="@id/showQrBottomSheet"
            app:popUpTo="@id/accountOptionsBottomSheet"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_accountOptionsBottomSheet_to_warningConfirmationNavigation"
            app:destination="@id/warningConfirmationNavigation"
            app:popUpTo="@id/accountOptionsBottomSheet"
            app:popUpToInclusive="true">
            <argument
                android:name="warningConfirmation"
                app:argType="com.algorand.android.models.WarningConfirmation" />
        </action>
    </dialog>

    <dialog
        android:id="@+id/dateFilterPickerBottomSheet"
        android:name="com.algorand.android.ui.datepicker.DateFilterListBottomSheet"
        android:label="DateFilterPickerBottomSheet"
        tools:layout="@layout/bottom_sheet_date_filter_list">
